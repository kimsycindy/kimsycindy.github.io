{
  "name": "cindy-kim-website",
  "description": "Cindy Kim Gatsby website",
  "version": "1.0.0",
  "private": true,
  "author": "Ryan Lee <hi@iamryanlee.com>",
  "keywords": [
    "gatsby",
    "react",
    "portfolio",
    "website"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "serve": "npm run build && gatsby serve",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "start": "npm run dev",
    "test": "jest",
    "test:update": "jest -u"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^13.4.0",
    "@contentful/rich-text-types": "^14.1.0",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "animate.css": "^4.1.1",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "gatsby": "^2.21.22",
    "gatsby-image": "^2.4.3",
    "gatsby-plugin-canonical-urls": "^2.3.1",
    "gatsby-plugin-google-analytics": "^2.3.1",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-sass": "^2.3.1",
    "gatsby-plugin-sharp": "^2.6.2",
    "gatsby-plugin-typescript": "^2.4.2",
    "gatsby-remark-copy-linked-files": "^2.3.2",
    "gatsby-remark-images": "^3.3.2",
    "gatsby-remark-responsive-iframe": "^2.4.2",
    "gatsby-remark-smartypants": "^2.3.1",
    "gatsby-source-contentful": "^2.3.3",
    "gatsby-source-filesystem": "^2.3.1",
    "gatsby-transformer-json": "^2.4.1",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^2.5.2",
    "lodash.groupby": "^4.6.0",
    "modern-normalize": "^0.5.0",
    "node-sass": "^4.14.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-image-lightbox": "^5.1.1",
    "react-images": "^1.1.7",
    "react-photo-gallery": "^8.0.0",
    "ts-is-present": "^1.1.1",
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "^10.0.4",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^25.2.1",
    "@types/lodash.groupby": "^4.6.6",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^5.0.8",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.32.0",
    "@typescript-eslint/parser": "^2.32.0",
    "babel-jest": "^26.0.1",
    "babel-preset-gatsby": "^0.4.1",
    "eslint": "^6.6.0",
    "eslint-config-blvd": "^1.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.2",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --cache --fix",
      "npm run test -- --passWithNoTests"
    ]
  }
}
